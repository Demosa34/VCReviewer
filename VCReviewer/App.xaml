<Application x:Class="VCReviewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VCReviewer"
             
              xmlns:viewmodel="clr-namespace:VCReviewer.ViewModel"
             StartupUri= "MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <viewmodel:LocatorViewModel x:Key="Locator"/>
                </ResourceDictionary>


                    <ResourceDictionary Source="Styles\Colors.xaml" />
                <ResourceDictionary Source="Styles\Fonts.xaml" />
                <ResourceDictionary Source="Styles\Icons.xaml" />
                <ResourceDictionary Source="Styles\Defaults.xaml" />
                <ResourceDictionary Source="Styles\Texts.xaml" />
                <ResourceDictionary Source="Styles\Buttons.xaml" />
                <ResourceDictionary Source="Styles\ContentControls.xaml" />
                <ResourceDictionary Source="Styles\UserControl.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
