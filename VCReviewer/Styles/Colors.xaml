<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:VCReviewer.Styles">


    <SolidColorBrush x:Key="DarkBaseBrush" Color="#FF0040FF" /> <!--#158150-->
    <Color x:Key="DarkBaseColor">#FF0040FF</Color>

    <SolidColorBrush x:Key="MediumBaseBrush" Color="#FF3C64DC" /> <!--#21a86d   base collor defult-->
    <Color x:Key="MediumBaseColor">#FF3C64DC</Color>

    <SolidColorBrush x:Key="LightBaseBrush" Color="#FF5F87F5" /> <!--#FF2CB679-->
    <Color x:Key="LightBaseColor">#FF5F87F5</Color>

    <SolidColorBrush x:Key="UltraLightBaseBrush" Color="#FFA0B9FF" /> <!--#FF3BC588-->
    <Color x:Key="UltraLightBaseColor">#FFA0B9FF</Color>


    <SolidColorBrush x:Key="DarkBrush" Color="#FFD6D6D6" />
    <Color x:Key="DarkColor">#FFD6D6D6</Color>

    <SolidColorBrush x:Key="MediumBrush" Color="#FFF1F1F1" />
    <Color x:Key="MediumColor">#FFF1F1F1</Color>

    <SolidColorBrush x:Key="LightBrush" Color="white" />
    <Color x:Key="LightColor">white</Color>




</ResourceDictionary>